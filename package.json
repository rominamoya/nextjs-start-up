{
    "name": "nextjs-start-up",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "prepare": "husky install",
        "lint-staged": "lint-staged",
        "test": "jest"
    },
    "dependencies": {
        "next": "15.3.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@typescript-eslint/eslint-plugin": "^8.32.0",
        "@typescript-eslint/parser": "^8.32.0",
        "eslint": "^9",
        "eslint-config-next": "15.3.1",
        "eslint-config-prettier": "^10.1.3",
        "eslint-plugin-prettier": "^5.4.0",
        "husky": "^8.0.0",
        "lint-staged": "^15.5.2",
        "prettier": "^3.5.3",
        "typescript": "^5"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,css,md}": [
            "prettier --write"
        ]
    }
}
